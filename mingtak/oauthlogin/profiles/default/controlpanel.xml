<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="mingtak.oauthlogin">

    <configlet
        title="Oauth2 settings"
        action_id="mingtak.oauthlogin.oauth2setting"
        appId="mingtak.oauthlogin"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@oauth2SettingControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
